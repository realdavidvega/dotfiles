<keymap version="1" name="macOS custom" parent="Mac OS X 10.5+">
  <action id="ActivateBigDataToolWindowToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl meta b" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="meta open_bracket" />
    <keyboard-shortcut first-keystroke="meta alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl open_bracket" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="meta close_bracket" />
    <keyboard-shortcut first-keystroke="meta alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="ctrl close_bracket" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
</keymap>